|  PRSTRING PRTITULOX ASIGNACION valor_barra:a PTCOMA
                                                {:  
                                                    grfBarras.tituloX = a.getString();
                                                    //RESULT = a;
                                                :}
|  PRSTRING PRTITULOY ASIGNACION valor_barra:a PTCOMA
                                                {:
                                                    grfBarras.tituloY = a.getString();
                                                    //RESULT = a;
                                                :}
;

valor_barra::=
    DECIMAL:a                                   {:
                                                    RESULT=new Production(new Node("dcm"), new Double(a));
                                                :}
 |  CADENA:a                                    {:
                                                    RESULT=new Production(new Node("str"), a.replace("\"", ""));
                                                :}
 | DOLAR LLAVIZQ NVALOR COMA CADENA:a COMA CADENA:b LLAVDER
                                                {: 
                                                    String nombre = a.replace("\"", "");
                                                    String id = b.replace("\"", "");
                                                    boolean encontrado = false;
                                                    for(ObjetoJSON obj: Analizador.objetosCargados) {
                                                        if(obj.nombreArchivo.equals(nombre)) {
                                                            if(obj.id.equals(id)) {
                                                                if(obj.valorCadena != null) {
                                                                    RESULT=new Production(new Node("valor"), obj.valorCadena);
                                                                } else {
                                                                    RESULT=new Production(new Node("vDouble"), obj.valorDouble) ;
                                                                }
                                                                encontrado = true;
                                                                break;
                                                            }
                                                        }
                                                    }
                                                    if(!encontrado) System.out.println("NO ENCONTRADO");
                                                :}
 |  IDENTIFICADOR:a                             {:
                                                    boolean encontrado = false;
                                                    for(VariableGlobal varG: varGlobales) {
                                                        if(varG.id.equals(a)) {
                                                            encontrado = true;
                                                            if(varG.valor != null) {
                                                                RESULT=new Production(new Node("valor"), varG.valor);
                                                            } else {
                                                                RESULT=new Production(new Node("vDouble"), varG.vDouble) ;
                                                            }
                                                            break;
                                                        }
                                                    }
                                                    if(!encontrado) System.out.println("NO ENCONTRADO");
                                                :}
;

//getLeyendas() -> Obtiene la lista de leyendas en String.
leyendas ::=
    leyendas:a valor_barra:b            {:          
                                            RESULT=new Production(new Node("leyendas"), a.getLeyendas(), 0);
                                            RESULT.addLeyenda(b.getString());
                                        :}
    | leyendas:a valor_barra:b COMA     {:           
                                            RESULT=new Production(new Node("leyendas"), a.getLeyendas(), 0);
                                            RESULT.addLeyenda(b.getString());
                                        :}
    | valor_barra:a COMA                {:                                                                                                  
                                            RESULT=new Production(new Node("leyendas"), new LinkedList<String>(), 0);
                                            RESULT.addLeyenda(a.getString());
                                        :}
;

lista_valores ::=
    lista_valores:a valor_barra:b       {:   
                                            RESULT=new Production(new Node("valores"), a.getValores(), false);
                                            RESULT.addValor(b.getDouble());                                                                             
                                        :}
    | lista_valores:a valor_barra:b COMA {:                                                                                
                                            RESULT=new Production(new Node("valores"), a.getValores(), false);
                                            RESULT.addValor(b.getDouble());  
                                        :}
    | valor_barra:a COMA                  {:                                                                                                  
                                            RESULT=new Production(new Node("valores"), new LinkedList<Double>(), false);
                                            RESULT.addValor(a.getDouble());
                                        :}
;

carPie::=    
PRSTRING PRTITULO ASIGNACION valor_barra:a PTCOMA
                                                {:
                                                    grfPie.titulo = a.getString(); //Obtiene el string del valor barra
                                                    //RESULT = a;
                                                :}
|  PRSTRING CORIZQ CORDER PREJEX ASIGNACION LLAVIZQ leyendas:a LLAVDER PTCOMA
                                                {:
                                                    //RESULT = a.getLeyendas(); //Se obtiene la lista de leyendas
                                                    grfPie.listaEjeX = a.getLeyendas();
                                                :}
|  PRDOUBLE CORIZQ CORDER PRVALORES ASIGNACION LLAVIZQ lista_valores:a LLAVDER PTCOMA    
                                                {:
                                                    //RESULT = a.getValores(); //Se obtiene la lista de valores
                                                    grfPie.listaValores = a.getValores();
                                                :}
;

casos::= 
	casos:l PRCASE expresion:a DOSPUNTOS instrucciones:b	{:                                                                                
                                                                    RESULT=new Production(new Node("casos"), l.getInstructions());
                                                                    //Switch(null, expresion, null, instruccionesCaso);
                                                                    RESULT.addIns(new Switch(null,(Operacion)a.getIns(),null,b.getInstructions()));                                                    
                                                                    RESULT.addSon("casos");
                                                                    RESULT.addSon(l);
                                                                    RESULT.addSon("case");
                                                                    RESULT.addSon(a);
                                                                    RESULT.addSon(":");
                                                                    RESULT.addSon(b);
                                                                :} 
 | PRCASE expresion:a DOSPUNTOS instrucciones:b                  {:                                                                                
                                                                    RESULT=new Production(new Node("casos"), new LinkedList<>());
                                                                    RESULT.addIns(new Switch(null,(Operacion)a.getIns(),null,b.getInstructions())); 
                                                                    RESULT.addSon("case");
                                                                    RESULT.addSon(a);
                                                                    RESULT.addSon(":");
                                                                    RESULT.addSon(b);
                                                                :}
;

